		"astro": "^5.16.11",
		"date-fns": "^4.1.0",
		"eslint": "^10.0.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-astro": "^1.6.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"markdown-it": "^14.1.0",
		"prettier": "^3.8.1",
		"prettier-plugin-astro": "^0.14.1",
		"sanitize-html": "^2.17.0",
		"typescript-eslint": "^8.56.1"
	},
	"dependencies": {
		"@astrojs/node": "^9.5.2",
