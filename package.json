{
	"engines": {
		"node": ">=22"
	},
	"name": "msd",
	"type": "module",
	"version": "0.0.9",
	"scripts": {
		"dev": "astro dev",
		"start": "node ./dist/server/entry.mjs",
		"build": "astro build",
		"preview": "astro preview",
		"astro": "astro",
		"lint": "eslint .",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx}": [
